#!/bin/sh

. $(dirname $0)/system.conf


echo "==================================="
echo "====  QEMU: u-boot is starting ===="
echo "====  Press Ctrl+A+X to exit   ===="
echo "==================================="
echo ""

sleep 1

$qemu_system_arm -machine   vexpress-a9   \
                 -m         512M          \
                 -kernel    $uboot_image  \
                 -nographic
